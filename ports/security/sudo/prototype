i pkginfo=./pkginfo
i postinstall=./postinstall
d none bin 0775 root bin
f none bin/sudo 4111 root root
d none man 0755 bin bin
d none man/man5 0755 bin bin
f none man/man5/sudoers.5 0444 bin bin
d none man/man8 0755 bin bin
f none man/man8/sudo.8 0444 bin bin
f none man/man8/visudo.8 0444 bin bin
d none sbin 0755 bin bin
f none sbin/visudo 0111 root root
d none share/examples 0755 bin bin
d none share/examples/sudo 0755 bin bin
f none share/examples/sudo/sudoers 0644 bin bin
