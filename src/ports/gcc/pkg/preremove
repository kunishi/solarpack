INFODIR=$CLIENT_BASEDIR/info
INSTALL_INFO=$CLIENT_BASEDIR/bin/install-info

if test -x ${INSTALL_INFO}; then
	${INSTALL_INFO} --delete --info-dir=${INFODIR} ${INFODIR}/cpp.info
	${INSTALL_INFO} --delete --info-dir=${INFODIR} ${INFODIR}/gcc.info
else
	/usr/bin/echo ">> Remove cpp.info and gcc.info entry from ${INFODIR}/dir by hand."
fi