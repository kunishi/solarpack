i pkginfo=./pkginfo
d none bin 0755 bin bin
f none bin/gzip 0755 bin bin
f none bin/zdiff 0755 bin bin
f none bin/zgrep 0755 bin bin
f none bin/zmore 0755 bin bin
f none bin/znew 0755 bin bin
f none bin/zforce 0755 bin bin
f none bin/gzexe 0755 bin bin
l none bin/zcmp=zdiff
l none bin/gunzip=gzip
l none bin/zcat=gzip
d none info 0755 bin bin
f none info/gzip.info 0644 bin bin
d none man 0755 bin bin
d none man/man1 0755 bin bin
f none man/man1/gzip.1 0644 bin bin
f none man/man1/gzexe.1 0644 bin bin
f none man/man1/zdiff.1 0644 bin bin
f none man/man1/zgrep.1 0644 bin bin
f none man/man1/zmore.1 0644 bin bin
f none man/man1/znew.1 0644 bin bin
f none man/man1/zforce.1 0644 bin bin
l none man/man1/zcat.1=gzip.1
l none man/man1/zcmp.1=zdiff.1
l none man/man1/gunzip.1=gzip.1
